// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

bool search(int *a,int d)
{
    int n = sizeof(a)/sizeof(a[0]);
    
    for(int i=0;i<n;i++)
    {
        if(*(a+i) == d)
        {
            return true;
        }
    }
    return false;
}

int main() 
{
    int a[] = {10,20,30,40,50};
    int d = 20;
    int n = sizeof(a)/sizeof(a[0]);
    if(search(a,d))
    {
        cout<<"Present";
    }
    else
    {
        cout<<"Not Present";
    }

    return 0;
}
